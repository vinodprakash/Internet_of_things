<div class="row">
  <div class="small-2 large-6 columns">
<h1>Listing mobiles</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Model</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    
    <% @mobiles.each do |mobile| %>
      <tr>
        <td><%= mobile.name %></td>
        <td><%= mobile.model %></td>
        <td><%= link_to 'Show', mobile %></td>
        <td><%= link_to 'Edit', edit_mobile_path(mobile) %></td>
        <td><%= link_to 'Destroy', mobile, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Mobile', new_mobile_path %>

<br>
<br>
<a href="/user/index"><input type="button" value="List of user's"><a>

</div>
</div>
